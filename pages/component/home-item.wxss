.home-item-box{
  margin: 17rpx 0rpx;
  background: #fff;
  padding-bottom: 10rpx;
  border: 1px solid #f6f6f6;
  width: 350rpx;
  border-radius: 10px;
}
.home-item-img{
  height: 216rpx;
  border-radius: 10px 10px 0px 0px;

}
.home-item-name{
  padding-left: 10rpx;
  font-size: 30rpx;
}
.home-item-price{
  padding-left: 20rpx;
  color: #ff0000;
}
.home-item-discount-price{
  color: #999;
  text-decoration:line-through;
  font-size: 30rpx;
  padding-left: 10rpx;
}